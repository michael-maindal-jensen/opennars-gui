// === Basic subject predicate
// ex:"tim eats ice"
<(&&,<$V --> VERB>,<(*,$N1,$V,$N2) --> PART>) ==> <(*,(/,REPRESENT,$N1,_),(/,REPRESENT,$N2,_)) --> (/,REPRESENT,$V,_)>>.
//For determiners:
<(&&,<#a --> DET>,<(*,$N1,$V,#a,$N2) --> PART>) ==> <(*,(/,REPRESENT,$N1,_),(/,REPRESENT,$N2,_)) --> (/,REPRESENT,$V,_)>>.
<(&&,<#a --> DET>,<(*,#a,$N1,$V,$N2) --> PART>) ==> <(*,(/,REPRESENT,$N1,_),(/,REPRESENT,$N2,_)) --> (/,REPRESENT,$V,_)>>.
<(&&,<#a --> DET>,<(*,$N1,$V,#a,$N2) --> PART>) ==> <(*,(/,REPRESENT,$N1,_),(/,REPRESENT,$N2,_)) --> (/,REPRESENT,$V,_)>>.

// === and
// ex:"tim eats ice and beef"
<(&&,<$V --> VERB>,<(*,$N1,$V,$N2,"and",$N3) --> PART>) ==> (|, <(*,(/,REPRESENT,$N1,_),(/,REPRESENT,$N2,_)) --> (/,REPRESENT,$V,_)>, <(*,(/,REPRESENT,$N1,_),(/,REPRESENT,$N3,_)) --> (/,REPRESENT,$V,_)>)>.

// ex:"a car is red and mobile"
<(&&,<#a --> DET>,<(*,#a,$N1,$V,$N2,"and",$N3) --> PART>) ==> (|, <(*,(/,REPRESENT,$N1,_),(/,REPRESENT,$N2,_)) --> (/,REPRESENT,$V,_)>, <(*,(/,REPRESENT,$N1,_),(/,REPRESENT,$N3,_)) --> (/,REPRESENT,$V,_)>)>.

//With prepositions:
$0.1;0.9$ <(&&,<#in --> PLACE_PREP>,<(*,#in,$PLACE) --> PART>) ==> <(/,REPRESENT,$PLACE,_) --> PLACE>>.
$0.1;0.9$ <(&&,<#at --> TIME_PREP>,<(*,#at,$TIME) --> PART>) ==> <(/,REPRESENT,$TIME,_) --> TIME>>.
//Subject or object:
<(&&,<#a --> DET>,<(*,#a,$NOUN) --> PART>) ==> <(/,REPRESENT,$NOUN,_) --> IT>>.
//Determiners:
<"a" --> DET>.
<"an" --> DET>.
<"the" --> DET>.
//Prepositions:
<"in" --> PLACE_PREP>.
<"at" --> TIME_PREP>.
<"at" --> PLACE_PREP>.
<"on" --> PLACE_PREP>.
<"to" --> PLACE_PREP>.
//Verbs:
<"is" --> VERB>.
<"are" --> VERB>.
<"were" --> VERB>.
<"was" --> VERB>.
<"likes" --> VERB>.
<"hates" --> VERB>.
<"eats" --> VERB>.
<"walks" --> VERB>.
<"goes" --> VERB>.
<"drinks" --> VER>.
//Question words
<"what" --> QWORD>.
<"who" --> QWORD>.
<"where" --> QWORD>.
<"when" --> QWORD>.
